{
  "name": "@gqlts/runtime",
  "version": "3.2.10",
  "description": "Gqlts runtime client",
  "keywords": [
    "gqlts",
    "graphql",
    "axios",
    "typescript",
    "types-graphql",
    "types-graphql-client",
    "types-graphql-axios",
    "graphql-sdk",
    "graphql-sdk-generator",
    "graphql-sdk-cli",
    "graphql-subscriptions",
    "graphql-subscriptions-client",
    "graphql-typescript",
    "graphql-axios-client",
    "graphql-typescript-client",
    "graphql-typescript-sdk"
  ],
  "homepage": "https://gqlts.vercel.app",
  "repository": {
    "url": "https://github.com/meabed/gqlts"
  },
  "license": "MIT",
  "author": "Meabed <mo@meabed.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist/*",
    "/esm/*",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist esm *.tsbuildinfo && cp ./package.json src/version.json && tsc && tsc -m ESNext --outDir esm",
    "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
    "start": "tsdx watch",
    "test": "yarn typesTests && jest",
    "typesTests": "tsc -b src/client/__tests__/tsconfig.json",
    "dev": "yarn compile && node dist"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "mocha": {
    "require": "tests/init.js",
    "spec": "tests/**.ts",
    "timeout": 9999999999
  },
  "dependencies": {
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "graphql-ws": "^5.11.2",
    "is-plain-obj": "^4.1.0",
    "lodash.assign": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.startswith": "^4.2.1",
    "ws": "^8.9.0",
    "zen-observable-ts": "^1.1.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "3.0.0",
    "@types/common-tags": "1.8.1",
    "@types/jest": "29.1.2",
    "@types/lodash": "4.14.186",
    "@types/lodash.assign": "4.2.7",
    "@types/lodash.get": "4.4.7",
    "@types/lodash.startswith": "4.2.7",
    "@types/node": "18.8.3",
    "@types/qs": "6.9.7",
    "@types/ws": "8.5.3",
    "common-tags": "1.8.2",
    "dotenv": "16.0.3",
    "jest": "29.1.2",
    "sucrase": "3.28.0",
    "ts-jest": "29.0.3",
    "tslib": "2.4.0",
    "utility-types": "3.10.0"
  },
  "_": "[bump]"
}
